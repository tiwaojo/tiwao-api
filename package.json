{
  "name": "tiwao-graphql-api",
  "private": true,
  "version": "0.0.0",
  "description": "A GraphQL API using Prisma and MongoDB",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "codegen": "graphql-codegen",
    "seed": "ts-node-dev --transpile-only prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@prisma/client": "^5.11.0",
    "graphql": "^16.8.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.5",
    "prisma": "^5.11.0",
    "prisma-yup-generator": "^0.2.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.91",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "nexus-validate": "^1.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}