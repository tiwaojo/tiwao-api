{
  "name": "tiwao-api",
  "private": true,
  "version": "1.0.0",
  "description": "A GraphQL API using Prisma and MongoDB",
  "main": "dist/graphql/index.js",
  "scripts": {
    "dev": "npx dotenv -e .env.dev -- ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "build": "npm run prisma:generate && tsc -p ./tsconfig.json && npm run codegen",
    "clean": "rimraf dist",
    "prestart": "pnpm run clean && pnpm run prisma:generate && pnpm run build",
    "dev:func": "pnpm run prestart && npx dotenv -e .env.dev --  func start",
    "prod:app": "pnpm run prestart && npx dotenv -e .env.stage --  node dist/graphql/index.js",
    "start": "node dist/graphql/index.js",
    "codegen": "graphql-codegen --config codegen.ts",
    "seed": "npx dotenv -e .env.dev -- ts-node-dev --transpile-only prisma/seed.ts",
    "seed:stage": "pnpm run prisma:push-stage && npx dotenv -e .env.stage -- ts-node-dev --transpile-only prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push-dev": "npx dotenv -e .env.dev -- prisma db push",
    "prisma:push-stage": "npx dotenv -e .env.stage -- prisma db push",
    "prisma:studio": "npx dotenv -e .env.dev -- prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@apollo/server-plugin-response-cache": "^4.1.3",
    "@apollo/utils.keyvadapter": "^3.1.0",
    "@apollo/utils.keyvaluecache": "^3.1.0",
    "@as-integrations/azure-functions": "^0.1.3",
    "@azure/functions": "^3.5.1",
    "@keyv/compress-gzip": "^1.2.3",
    "@keyv/redis": "^2.8.5",
    "@pothos/core": "^3.41.2",
    "@pothos/plugin-prisma": "^3.65.3",
    "@pothos/plugin-validation": "^3.10.2",
    "@pothos/plugin-with-input": "^3.10.2",
    "@prisma/client": "^5.16.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@vercel/kv": "^1.0.1",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.9.0",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.23.0",
    "graphql-shield": "^7.6.5",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^4.5.4",
    "prisma": "^5.16.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.6",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@types/node": "^18.19.39",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}