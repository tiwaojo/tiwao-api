{
  "name": "tiwao-graphql-api",
  "private": true,
  "version": "0.0.0",
  "description": "A GraphQL API using Prisma and MongoDB",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "codegen": "graphql-codegen",
    "seed": "ts-node-dev --transpile-only prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "npx dotenv -e .env.dev -- prisma db push",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.65.0",
    "@pothos/plugin-validation": "^3.10.1",
    "@pothos/plugin-with-input": "^3.10.1",
    "@prisma/client": "^5.11.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "nexus": "^1.3.0",
    "prisma": "^5.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@pothos/plugin-authz": "^3.5.8",
    "@types/node": "^16.18.91",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "nexus-validate": "^1.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}